<?xml version="1.0"?>
<Container version="2">
  <Name>netiso</Name>
  <Repository>ghcr.io/tuxuser/netiso-srv-rs:latest</Repository>
  <Registry/>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/tuxuser/netiso-srv</Project>
  <Overview>Xbox 360 ISO games over a network using Aurora 0.7b&#xD;
https://consolemods.org/wiki/Xbox_360:Playing_Games_over_Network_(NetISO)</Overview>
  <Category>GameServers:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://logoeps.com/wp-content/uploads/2013/06/xbox-360-games-vector-logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1757418308</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="PORT" Target="" Default="4323" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">4323</Config>
  <Config Name="ISOs" Target="/mnt" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/Games</Config>
  <TailscaleStateDir/>
</Container>
