  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1581836166</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>OpenProject is a web-based project management system for location-independent team collaboration.</Description>
  <Networking>
    <Mode>br0</Mode>
    <Publish/>
