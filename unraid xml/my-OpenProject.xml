<?xml version="1.0"?>
<Container version="2">
  <Name>OpenProject</Name>
  <Repository>openproject/community:10</Repository>
  <Registry>https://hub.docker.com/u/openproject</Registry>
  <Network>br0</Network>
  <MyIP></MyIP>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
